{
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "transform-flow-strip-types"
      ]
    },
    "test": {
      "presets": ["es2015", "react", "stage-2"]
    }
  },
  "presets": [
    ["es2015", {"loose": true, "modules": false}],
    "react",
    "stage-2"
  ]
}
